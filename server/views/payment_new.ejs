<form action="/api/Prepaid" method="post">
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Nombre del Cliente:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-briefcase"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Referencia Pago:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-tag"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Fecha del pago:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-calendar"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Número de Atenciones:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-check"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Vigencia:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-calendar"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Nombre del contacto:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-user"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Teléfono de contacto:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-phone"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group col-sm-6">
    <label class="control-label" for="company">Correo de Contacto:</label>
    <div class="input-group">
      <span class="input-group-addon">
        <i class="glyphicon glyphicon-envelope"></i>
      </span>
      <input id="company" class="form-control" name="company" />
    </div>
  </div>
  <div class="form-group">
    <button class="btn btn-success" type="submit">Guardar y Enviar Recibo</button>
  </div>
</form>
